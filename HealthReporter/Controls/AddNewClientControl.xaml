<UserControl x:Class="HealthReporter.Controls.AddNewClientControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HealthReporter.Controls"
             mc:Ignorable="d" Height="350" Width="350">
    <Border BorderBrush="Black" BorderThickness="2">
        <Grid>
            <TextBox x:Name="firstName" HorizontalAlignment="Left" Height="23" Margin="144,72,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" />
            <TextBox x:Name="lastName" HorizontalAlignment="Left" Height="23" Margin="144,108,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
            <TextBlock x:Name="Text1" HorizontalAlignment="Left" Margin="58,72,0,0" TextWrapping="Wrap" Text="firstName" VerticalAlignment="Top"/>
            <TextBlock x:Name="Text2" HorizontalAlignment="Left" Margin="58,109,0,0" TextWrapping="Wrap" Text="lastName" VerticalAlignment="Top"/>
            <Button x:Name="button1" Content="Create" HorizontalAlignment="Left" Margin="244,181,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.329,-0.053" Click="btn_CreateNewClient"/>
            <Button x:Name="button" Content="back" HorizontalAlignment="Left" Margin="157,181,0,0" VerticalAlignment="Top" Width="75" Click="btn_Back"/>
        </Grid>
    </Border>
</UserControl>
